{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "env": { "browser": true, "es6": true },
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": [0],
    "no-console": "warn"
  }
}
